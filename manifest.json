{
    "name": "Calendar",
    "description": "This is an easy way to get your day to day events",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["src/content.js"]
        }
    ],
    "background": {
        "service_worker": "src/background.js"
    },
    "action": {
        "default_icon": "picture/pngtree-flat-calendar-image_1287503.jpg"
    },
    "permissions": ["storage", "notifications"]
}